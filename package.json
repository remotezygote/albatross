{
  "name": "@remotezygote/albatross",
  "version": "2.0.5",
  "description": "Migrator",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "author": "John Bragg",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "lint": "tsc --project tsconfig.json --noEmit",
