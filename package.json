    "build": "nanobundle build --tsconfig=./tsconfig.json --platform=node --out-dir=./dist --clean",
    "prepare": "yarn build",
    "release": "standard-version",
    "new": "node dist/index.cjs add"
  },
  "dependencies": {
    "@remotezygote/database": "^1.2.14",
    "@remotezygote/koa-api-app": "^1.0.40",
    "colors": "^1.4.0",
    "commander": "^12.1.0",
    "debug": "^4.3.5",
    "fast-glob": "^3.3.2",
    "kanel": "^3.9.0",
    "koa": "^2.15.3",
    "pg-protocol": "^1.6.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/koa": "^2.15.0",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.10.6",
    "dotenv": "^16.4.5",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-file-progress": "^1.4.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "nanobundle": "^2.1.0",
    "prettier": "^3.3.2",
    "standard-version": "^9.5.0",
    "typescript": "^5.5.3"
  },
  "files": [
    "./dist",
    "./src/install/migrations",
    "albatross"
  ],
  "bin": {
    "albatross": "./dist/index.cjs"
