    "koa-mount": "^4.0.0",
    "koa-pino-logger": "^4.0.0",
    "koa-static": "^5.0.0",
    "pg": "^8.8.0",
    "zapatos": "^6.1.4",
    "pg-native": "^3.0.1"
  },
  "devDependencies": {
    "@types/koa": "^2.13.5",
